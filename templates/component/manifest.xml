<?xml version="1.0" encoding="utf-8"?>
<updates>
    <update>
        <name>{{ sentenceCase componentName }}</name>
        <description><![CDATA[{{ constantCase componentDescription }}]]></description>
        <element>{{ lowerCase componentName }}</element>
        <type>component</type>
        <version>{{ version }}</version>
        <client>site</client>
        <infourl title="{{ sentenceCase componentName }}"></infourl>
        <downloads>
            <downloadurl type="full" format="zip"></downloadurl>
        </downloads>
        <targetplatform name="joomla" version="3.[6789]" />
    </update>
</updates>
